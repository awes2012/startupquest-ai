# Root environment variables (copy to .env for local dev)

# Database (Postgres)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/startupquest

# Redis
REDIS_URL=redis://localhost:6379

# Auth / Security
NEXTAUTH_SECRET=replace_me_with_strong_random
JWT_SECRET=replace_me_with_strong_random

# Storage (Localstack S3 or real S3)
S3_ENDPOINT=http://localhost:4566
S3_REGION=us-east-1
S3_BUCKET=startupquest-dev

# AI Providers (optional in dev)
OPENAI_API_KEY=

